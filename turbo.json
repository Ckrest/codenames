{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "serve": {
      "cache": false
    },
    "deploy": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
