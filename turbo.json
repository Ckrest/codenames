{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "codenames-frontend#build": {
      "outputs": ["dist/**", "www/**"]
    },
    "codenames-backend#build": {
      "outputs": ["build/**"]
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "serve": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
